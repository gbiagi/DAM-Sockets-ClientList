<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox prefHeight="32.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.client.ClientController">
   <children>
      <Text fx:id="name" wrappingWidth="421.0">
         <font>
            <Font size="24.0" />
         </font>
         <HBox.margin>
            <Insets left="5.0" />
         </HBox.margin></Text>
      <Button fx:id="bt1" text="▲">
         <HBox.margin>
            <Insets right="5.0" top="3.0" />
         </HBox.margin>
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="bt2" text="▼">
         <HBox.margin>
            <Insets right="5.0" top="3.0" />
         </HBox.margin>
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="bt3" text="Edit">
         <HBox.margin>
            <Insets right="5.0" top="3.0" />
         </HBox.margin></Button>
      <Button fx:id="bt4" text="Delete">
         <HBox.margin>
            <Insets right="5.0" top="3.0" />
         </HBox.margin></Button>
   </children>
</HBox>
